  "skripts": {
    "start": "./node_modules/.bin/cypress open",
    "cypress:open": "yarn run cypress open --browser chrome",
    "cypress:run": "cypress run",
    "cypress:run:ci": "cypress run --reporter mochawesome --reporter-options reportDir=reports,overwrite=false,html=false,json=true",
    "cypress:run:ci:merge": "mochawesome-merge reports/mochawesome*.json > reports/mochawesome.json",
    "cypress:run:ci:report": "marge reports/mochawesome.json -f report -o reports"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "cypress": "^12.8.1",
    "cypress-file-upload": "6.0.7",
    "cypress-multi-reporters": "1.7.0",
    "cypress-plugin-tab": "1.0.7",
    "cypress-xpath": "1.6.2",
    "lodash": "4.17.21",
    "mocha": "8.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "4.0.0",
    "husky": "3.1.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "6.1.1",
    "prettier": "2.5.1"
  }
